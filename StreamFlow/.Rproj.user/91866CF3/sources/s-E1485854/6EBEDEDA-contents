compute.variance <- function(x){
  x.bar <- mean(x)
  n <- length(x)
  variance <- (sum(x-x.bar))^2/(n-1)
  #variance
}
my.function <- function(z) {
  m <- Inf
  for (i in 1:length(z))
    if (z[i]<m)
      m <- z[i]
    m
}

y_hat <- function(X,y){
  if(length(X)!=length(y)){
    error
  }
  y.hat <- solve(t(X)*X)*t(X)*y
  return(list(y.hat,X,y))
}

?unique
